package sayiBulma;

import java.util.Scanner;

public class Main {

	public static void main(String[] args) {
		int[] myList= {1,2,3,4,5,6,7,8,9,0};
		Scanner bilgi=new Scanner(System.in);
		System.out.println("Aramak istediðiniz rakamý girin: ");
		int aranacakSayi=bilgi.nextInt();
		
		for(int sayi:myList) {
			if(sayi==aranacakSayi) {
				System.out.println("Aradýðýnýz sayý dizide mevcut.");
			}
			else {
				System.out.println("Aradýðýnýz sayý dizide bulunamadý");
			}
		}

	}

}
