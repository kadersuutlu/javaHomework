package diziElemanlarýToplamý;

public class Main {

	public static void main(String[] args) {
		double[] myList= {1.2,1.3,4.3,5.6};
		double max=myList[0];
		double total=0;
		for(double number:myList) {
			System.out.println("Liste elemanlarý: "+number);
			if(max>number) {
				max=number;	
			}
			total=total+number;	
		}
		System.out.println("En büyük sayý: "+max);
		System.out.println("Topam: "+total);
	}
}
